func removeUnderscore(name string) string => replace(name, '_', '')
func removeSpecialCharacters(name string) string => replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(name, '!', ''), '@', ''), '#', ''), '$', ''), '%', ''), '^', ''), '&', ''), '*', ''), '(', ''), ')', ''), ',', ''), '.', ''), '/', ''), '\\', ''), '-', ''), '+', ''), '=', ''), '|', '')
