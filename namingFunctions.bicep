
@export()
func cleanContainerRegistryName(name string) string => removeNonAlphaNumerics(name)

@export()
func cleanCosmosName(name string) string => trimNonAlphaNumericFromFront(toLower(removeSpecialCharacters(name)))

@export()
func cleanUamiName(name string) string => trimNonAlphaNumericFromFront(removeSpecialCharacters(removeUnderscore(name)))

@export()
func cleanKeyVaultName(name string) string => removeConsecutiveHypens(trimNonAlphaNumericFromEnd(removeSpecialCharacters(name)))

